{
  "name": "vpofone",
  "version": "1.0.0",
  "description": "Executive leverage for solo leaders",
  "scripts": {
    "orchestrate": "node scripts/agent-orchestrator.js",
    "background": "echo \"Background agent started with prompt:\" && echo $CURSOR_BACKGROUND_AGENT_PROMPT",
    "dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "dev:backend": "cd backend && npm run dev",
    "start:backend": "cd backend && npm start",
    "install:backend": "cd backend && npm install",
    "migrate": "cd backend && npm run migrate",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "test:unit": "./scripts/test.sh unit",
    "test:integration": "./scripts/test.sh integration",
    "test:e2e": "./scripts/test.sh e2e",
    "test:all": "./scripts/test.sh all",
    "test:coverage": "jest --coverage",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "docker:test": "docker-compose -f docker-compose.test.yml up",
    "docker:build": "docker-compose build",
    "deploy:production": "./scripts/deploy.sh production all",
    "deploy:staging": "./scripts/deploy.sh staging all",
    "deploy:frontend": "./scripts/deploy.sh production frontend",
    "deploy:backend": "./scripts/deploy.sh production backend",
    "migrate:dev": "cd backend && npm run migrate:dev",
    "migrate:deploy": "cd backend && npm run migrate:deploy",
    "type-check": "tsc --noEmit",
    "prepare": "husky install"
  },
  "keywords": [
    "vp",
    "of",
    "one"
  ],
  "author": "Utlyze",
  "license": "MIT"
}